{
    "name": "vigihdev/support",
    "version": "1.0.0",
    "description": "A robust set of utility and helper classes for PHP development, including string manipulation and array helpers.",
    "type": "library",
    "keywords": [
        "php",
        "support",
        "utility",
        "helpers",
        "string-manipulation",
        "casing"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/string": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\Support\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev"
    },
    "scripts": {
        "test": "phpunit",
        "server": "php -S localhost:1934 server.php"
    }
}